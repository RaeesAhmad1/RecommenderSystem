    <script>
        document.title = "Recommender System";

        function isNullOrEmpty(s) {
            return (s == null || s === "");
        }

        function readURL(input) { ''
            var id = $(input).attr("data-src");
            if (input.files && input.files[0]) {
                var reader = new FileReader();
                reader.onload = function (e) {
                    $(id).attr("src", e.target.result);
                }
                reader.readAsDataURL(input.files[0]);
            }
        }

        function htmlEncode(value) {
            var encodedValue = $('<div />').text(value).html();
            return encodedValue;
        }

        function Notify(message, position = "top-right") {
            var theme = "";
            if (message.MessageType == "Success") {
                theme = "bg-green";
            } else if (message.MessageType == "Error") {
                theme = "bg-red";
            } else if (message.MessageType == "Warning") {
                theme = "bg-orange";
            } else if (message.MessageType == "Info") {
                theme = "bg-blue-alt";
            }

            $.toast({
                heading: message.MessageType,
                text: message.Description,
                showHideTransition: 'plain',
                icon: message.MessageType.toLowerCase(),
                position: 'bottom-right',
            });

        }

        window.Handlebars.registerHelper('select', function (value, options) {
            var $el = $('<select />').html(options.fn(this));
            $el.find('[value="' + value + '"]').attr({ 'selected': 'selected' });
            return $el.html();
        });



        $(function () {
            var identifier = '@identifier';
            var notification = $.connection.notification;

            $('.notification-bell').click(function () {
                $(this).find('.notification-menu').toggle();
        });

            $('.datepicker').bsdatepicker({
                format: 'mm-dd-yyyy'
            });


            $('.DataTable').dataTable({ responsive: true });
            $('[data-toggle="tooltip"]').tooltip();
            $('.dataTables_filter input').attr("placeholder", "Search...");

            $('html').on('mouseup', function (e) {
                if (!$(e.target).closest('.popover').length) {
                    $('.popover').each(function () {
                        $(this.previousSibling).popover('hide');
                    });
                }
            });

            $(".ReadNotificationBtn").click(function () {
                var id = $(this).data("id");
                $.post("/Panel/MarkRead", { MessageID: id }).then(function () {
                    $("#Notification_" + id).remove();
                    if ($(".ReadNotificationBtn").length == 0) {
                        $("#NotificationBadge").hide();
                    }
                });
            });



            $(".select2").select2();

            $.connection.hub.qs = { 'username': identifier };
            notification.client.Notify = function (message) {
                if (message.IsAjaxMessage)
                {
                    Notify(message);
                }
            };

            $.connection.hub.start().done(function () {
                console.log("connection established");
                getNotifications();
            });
            notification.client.getNotifications = function () {
                getNotifications();
            };


            function getNotifications() {
                $.post('/Panel/BellNotificationListener').then(function (notificationCount) {
                    $("#NotificationsCount").html(notificationCount);
                });
            }

            //$(".ImageFile").change(function () {
            //    readURL(this);
            //});





            $("#BellNotificationIcon").click(function () {
                $("#BellNotificatonList").html('<img src="~/images/input-spinner.gif" />');
                $.post("/Panel/GetBellNotifications").then(function (result) {
                    AppendToNotificationList(result);
                });
            });

            function AppendToNotificationList(result) {
                var data = "";
                data += "<li>";
                data += "<div class='row'>"
                data += "<div class='col-md-6'>";
                data += "<p>Notifications</p>";
                data += "</div>";
                data += "<div class='col-md-6'>";
                data += "<div class='pull-right'>";
                data += "<p><a href='#/' style='color:#0000EE;'>Mark all as read >></a></p>";
                data += "</div>";
                data += "</div>";
                data += "</div>"
                data += "</li>";
                var theTemplate = window.Handlebars.compile($("#BellNotificationTemplate").html());
                $.each(result, function (ind, val) {
                    console.log(val);
                    var context = {
                        MessageType: (function () {
                            switch (val.MessageType) {
                                case "Success":
                                    return "success";
                                case "Error":
                                    return "danger";
                                case "Warning":
                                    return "warning";
                                case "Info":
                                    return "info";
                            }
                        })(),
                        MessageURL: val.URL,
                        MessageDescription: val.Description
                    };
                    data += theTemplate(context);
                });
                data += "<li>";
                data += "<div class='row'>"
                data += "<div class='col-md-6'>";
                data += "<p></p>";
                data += "</div>";
                data += "<div class='col-md-6'>";
                data += "<div class='pull-right'>"
                data += "<p><a href='/Panel/Notifications' style='color: #0000EE;'>View All >></a></p>";
                data += "</div>";
                data += "</div>";
                data += "</div>"
                data += "</li>";
                $("#BellNotificatonList").html(data);
            }


        });
    </script>
    @if (ViewBag.NotificationMessage != null)
    {
        <script>
            $(function () {
                var message = @(Html.Raw(Json.Encode(ViewBag.NotificationMessage)));
                if (message.IsViewMessage) {
                    Notify(message);
                }
            });
        </script>
    }
    @if (TempData["NotificationMessage"] != null)
    {
        <script>
            $(function () {
                var message = @(Html.Raw(Json.Encode(TempData["NotificationMessage"])));
                if (message.IsRedirectMessage) {
                    Notify(message);
                }
            });
        </script>
    }

    @RenderSection("ViewScript", false)
